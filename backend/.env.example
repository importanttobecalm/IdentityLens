# IdentityLens Backend Configuration

# API Keys
FAL_API_KEY=your_fal_api_key_here
REPLICATE_API_TOKEN=your_replicate_token_here

# Server Configuration
PORT=8000
HOST=0.0.0.0
LOG_LEVEL=info

# API Provider (fal | replicate | comfyui)
API_PROVIDER=fal

# Model Configuration
DEFAULT_MODEL=flux-schnell
ENABLE_HARMONIZATION=true
DENOISING_STRENGTH=0.40

# Performance
MAX_CONCURRENT_REQUESTS=10
REQUEST_TIMEOUT=30

# Security
CORS_ORIGINS=*
API_KEY_REQUIRED=false
# API_KEY=your_server_api_key  # Uncomment for production

# Monitoring (optional)
ENABLE_METRICS=false
SENTRY_DSN=

# ComfyUI (if using self-hosted)
COMFYUI_ENDPOINT=http://localhost:8188
